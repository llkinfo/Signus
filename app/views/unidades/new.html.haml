.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "Listar")}", unidades_path
      %li.active= link_to "#{t("web-app-theme.new", :default => "Novo")}", new_unidade_path
  .content
    %h2.title
      = t("web-app-theme.new", :default => "Novo")
      Unidade
    .inner
      = form_for :unidade, :url => unidades_path, :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'sidebar')
